{
  "name": "fadriqueclickennet/user-module",
  "type": "clickennet-module",
  "description": "User module for ClickennetCMS. Handles the authentication and authorisation as well as the user management.",
  "keywords": [
    "clickennet",
    "user",
    "authentication",
    "authorisation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "fadriqueclickennet",
      "email": "fadriqueclickennet@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "composer/installers": "~1.0",
    "ramsey/uuid": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5",
    "orchestra/testbench": "3.8.*",
    "scrutinizer/ocular": "~1.1",
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Modules\\User\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\User\\Tests\\": "Tests"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
